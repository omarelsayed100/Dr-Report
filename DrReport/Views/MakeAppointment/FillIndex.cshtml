
@{
    ViewData["Title"] = "FillIndex";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Medical Diagnosis</title>
    <!--bootstrap css link-->
    <link href="~/lib/bootstrap/dist/css/bootstrap.css" rel="stylesheet" />
    <!--Css Link-->
    <link href="~/Styles/FillAppointment/style.css" rel="stylesheet" />
</head>
<body>
    <div class="content">
        <div class="form">
            <div class="data">
                <h3>Fill Appointment Data</h3>
                <input type="text" name="search" placeholder="Choose Diagnosis Type" oninput="validateSymptoms()">
                <input type="date" id="Appointment" name="Appointment">
                <div class="check">
                    <span>Allow Traffic regulator.</span>
                    <input type="checkbox" name="terms" onchange="validateTerms()">
                </div>
                <div class="col-lg-10">
                    <button id="confirm" type="button" class="btn_confirm">Confirm</button>
                </div>

            </div>
        </div>
    </div>
    <script src="~/Styles/FillAppointment/script.js"></script>
</body>
</html>


